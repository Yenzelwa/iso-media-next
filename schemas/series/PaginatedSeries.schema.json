{
  "$id": "https://isolakwamuntu.com/schemas/series/PaginatedSeries.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["items", "page", "page_size", "total"],
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "$ref": "https://isolakwamuntu.com/schemas/series/Series.schema.json"
      }
    },
    "page": {
      "type": "integer",
      "minimum": 1
    },
    "page_size": {
      "type": "integer",
      "minimum": 1
    },
    "total": {
      "type": "integer",
      "minimum": 0
    },
    "next_cursor": {
      "type": ["string", "null"]
    }
  },
  "additionalProperties": false
}