{
  "$id": "https://isolakwamuntu.com/schemas/billing/BillingHistoryResponse.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["transactions"],
  "properties": {
    "transactions": {
      "type": "array",
      "items": {
        "$ref": "BillingTransaction.schema.json"
      },
      "description": "List of billing transactions"
    },
    "has_more": {
      "type": "boolean",
      "description": "Whether there are more transactions to fetch"
    }
  },
  "additionalProperties": false
}